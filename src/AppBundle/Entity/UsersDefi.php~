<?php

namespace AppBundle\Entity;

/**
 * UsersDefi
 */
class UsersDefi
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $sport;

    /**
     * @var integer
     */
    private $level;

    /**
     * @var string
     */
    private $type;

    /**
     * @var integer
     */
    private $time;

    /**
     * @var string
     */
    private $description;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set sport
     *
     * @param integer $sport
     *
     * @return UsersDefi
     */
    public function setSport($sport)
    {
        $this->sport = $sport;

        return $this;
    }

    /**
     * Get sport
     *
     * @return integer
     */
    public function getSport()
    {
        return $this->sport;
    }

    /**
     * Set level
     *
     * @param integer $level
     *
     * @return UsersDefi
     */
    public function setLevel($level)
    {
        $this->level = $level;

        return $this;
    }

    /**
     * Get level
     *
     * @return integer
     */
    public function getLevel()
    {
        return $this->level;
    }

    /**
     * Set type
     *
     * @param string $type
     *
     * @return UsersDefi
     */
    public function setType($type)
    {
        $this->type = $type;

        return $this;
    }

    /**
     * Get type
     *
     * @return string
     */
    public function getType()
    {
        return $this->type;
    }

    /**
     * Set time
     *
     * @param integer $time
     *
     * @return UsersDefi
     */
    public function setTime($time)
    {
        $this->time = $time;

        return $this;
    }

    /**
     * Get time
     *
     * @return integer
     */
    public function getTime()
    {
        return $this->time;
    }

    /**
     * Set descritption
     *
     * @param string $descritption
     *
     * @return UsersDefi
     */
    public function setDescritption($descritption)
    {
        $this->descritption = $descritption;

        return $this;
    }

    /**
     * Get descritption
     *
     * @return string
     */
    public function getDescritption()
    {
        return $this->descritption;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return UsersDefi
     */
    public function setDescription($description)
    {
        $this->description = $description;
    
        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }
}
